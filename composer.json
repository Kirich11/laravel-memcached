{
    "name": "kirich/laravelmemcached",
    "description": "laravel library for caching results in memcached",
    "keywords": ["memcached", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirichenko Nickita",
            "email": "kirich.na@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "laravel/framework": "^6.0|^7.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Kirich\\LaravelMemcached\\": "src/"
        }
    },
    "suggest": {
        "ext-memcached": "The extension required to use this pool."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kirich\\LaravelMemcached\\Providers\\MemcachedServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
